meta {
  name: Get Messages
  type: http
  seq: 1
}

get {
  url: {{SUPABASE_API_URL}}/functions/v1/data-api/v1/users/{{user_id}}/conversations/{{conversation_id}}/messages
  body: json
  auth: bearer
}

auth:bearer {
  token: {{SUPABASE_SERVICE_ROLE_KEY}}
}

body:json {
  {
      "organization_id": "{{organization_id}}",
      "bot_id": "{{bot_id}}",
      "input": "Where does Nathan work?",
      "stream": true,
      "accumulate": false,
      "full_conversation": false,
      "num_message_history": 5
  }
}

vars:pre-request {
  ~organization_id: 
  ~bot_id: 
  ~user_id: 
  ~conversation_id: 
}
