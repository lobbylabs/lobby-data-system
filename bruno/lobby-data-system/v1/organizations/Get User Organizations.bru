meta {
  name: Get User Organizations
  type: http
  seq: 2
}

get {
  url: {{SUPABASE_API_URL}}/functions/v1/data-api/v1/organizations/{{organization_id}}/users
  body: none
  auth: bearer
}

auth:bearer {
  token: {{SUPABASE_SERVICE_ROLE_KEY}}
}

vars:pre-request {
  ~organization_id: <set value and enable to overwrite default>
}
