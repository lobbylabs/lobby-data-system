meta {
  name: Create Bot
  type: http
  seq: 1
}

post {
  url: {{SUPABASE_API_URL}}/functions/v1/data-api/v1/organizations/{{organization_id}}/bots
  body: json
  auth: bearer
}

auth:bearer {
  token: {{SUPABASE_SERVICE_ROLE_KEY}}
}

body:json {
  {
      "user_id_owner": "{{user_id}}",
      "system_prompt": "you are a helpful bot"
  }
}

vars:pre-request {
  ~user_id: 
  ~organization_id: 
}
